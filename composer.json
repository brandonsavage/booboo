{
    "name": "league/booboo",
    "type": "library",
    "description": "Provides an error handler for PHP that can execute a stack of handlers for various purposes.",
    "license": "MIT",
    "authors": [
        {
            "name": "Brandon Savage",
            "homepage": "https://www.brandonsavage.net/"
        }
    ],
    "require": {
        "php": "^8.2 || ^8.3",
        "psr/log": "^1.0"
    },
    "suggest": {
        "sentry/sentry": "Use Sentry for passing errors to Sentry."
    },
    "autoload": {
        "psr-4": {
            "League\\BooBoo\\": "src/",
            "Tests\\Fakes\\": "tests/fakes/"
        }
    },
    "require-dev": {
        "mockery/mockery": "^1.4",
        "phpunit/phpunit": "^10.5.20",
        "php-http/mock-client": "^1.4",
        "sentry/sentry": "^3.3",
        "symfony/http-client": "^7.0",
        "nyholm/psr7": "^1.8"
    },
    "config": {
        "allow-plugins": {
            "php-http/discovery": true
        }
    }
}
